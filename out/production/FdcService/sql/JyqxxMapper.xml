        select * from t_jyqxx where f_id=#{id}
    </select>


    <select id="list" resultMap="my_jyqxx">
        select * from t_jyqxx where  1=1 order by f_create_time desc limit #{index},#{size}
    </select>

    <select id="searchByPage" parameterType="HashMap" resultMap="my_jyqxx">
        select *
        from t_jyqxx
        where 1=1
        <if test="qyName!=null and qyName!=''">
            and f_qy_name like CONCAT("%",#{qyName},"%")
        </if>
      <!--  <if test="status!=null and status!=''">
            and f_status = #{status}
        </if>-->
        <if test="xmmc!=null and xmmc!=''">
            and f_xmmc like CONCAT("%",#{xmmc},"%")
        </if>

        limit #{start},#{limit}
    </select>

    <select id="count4Page" parameterType="HashMap" resultType="Integer">
